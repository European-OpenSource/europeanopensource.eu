---
import { BaseLayout } from "@walle/layouts";
---

<BaseLayout
  headerTitle="Submit OpenSource project"
  headerDescription="Discover the mission and vision of European OpenSource."
  robots="noindex, nofollow"
>
  <script async src="https://tally.so/widgets/embed.js"></script>
  <iframe
    data-tally-src="https://tally.so/r/n0k9DZ?transparentBackground=1&hideTitle=1&hideLogo=1"
    width="100%"
    height="100%"
    title="Add opensource project"></iframe>
</BaseLayout>

<style>
  iframe {
    display: block;
    width: 100vw;
    height: 100vh;
    border: 0;
  }
</style>
